=render :partial => 'dashboard', :locales => { :user => @user }

#content.followed
  -@user.following.each do |venue|
    
    =image_tag venue.logo_url, {:width => "120px" }
    .followed-company-content
      %h3=venue.full_name
      %p=venue.description
      %p 
        Telefon:
        %strong=venue.phone_number
      %p=link_to venue.email, :mailto => venue.email
      %p
        =venue.zip_code + " " + venue.city
        %br
        =venue.address
      =link_to "Zobacz oferty z tego miejsca",  venue, :class => "venue"
    %br
    %hr
